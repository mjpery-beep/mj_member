{
    "name": "mjpery-beep/mj-member",
    "description": "Plugin MJ Member",
    "type": "wordpress-plugin",
    "license": "proprietary",
    "require": {
        "psr/container": "^2.0",
        "symfony/form": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/http-foundation": "^6.4",
        "symfony/options-resolver": "^6.4",
        "twig/twig": "^3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Mj\\Member\\": "includes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mj\\Member\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "platform": {
            "php": "8.3.23"
        }
    }
}
